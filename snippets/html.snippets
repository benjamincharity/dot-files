# Forms
#
snippet form
<form action="#" method="${1:GET}">
	${2}
</form>${3}
endsnippet
snippet fieldset
<fieldset>
	<legend>${1:name}</legend>
	${2}
</fieldset>${3}
endsnippet
snippet input
<input type="${1:text/submit/hidden/button}">${2}
endsnippet
snippet inputt
<input type="text" placeholder="${1}">${2}
endsnippet
snippet inpute
<input type="email" placeholder="${1}">${2}
endsnippet
snippet inputurl
<input type="url" placeholder="${1}">${2}
endsnippet
snippet label
<label for="${1:NAME}">${2:Label}</label>${3}
endsnippet
snippet select
